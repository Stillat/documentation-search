{
    "name": "stillat/documentation-search",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Stillat\\DocumentationSearch\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "tests/" }
    },
    "extra": {
        "statamic": {
            "name": "Documentation Search for Statamic",
            "description": "Documentation Search provides a custom Statamic Search provider, targeted at documentation websites."
        },
        "laravel": {
            "providers": [
                "Stillat\\DocumentationSearch\\ServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.1",
        "statamic/cms": "^4 || ^5",
        "laravel/pint": "^1.13",
        "stillat/statamic-template-resolver": "^1.1",
        "ext-dom": "*",
        "ext-mbstring": "*",
        "ext-libxml": "*"
    },
    "require-dev": {
        "pestphp/pest": "^2.24",
        "orchestra/testbench": "^8.14 || ^9",
        "pestphp/pest-plugin-laravel": "^2.2"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "pixelfear/composer-dist-plugin": true
        }
    }
}
